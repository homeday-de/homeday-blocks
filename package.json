{
  "name": "homeday-blocks",
  "version": "0.1.3",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build:lib": "vue-cli-service build --target lib --name homeday-blocks ./src/components/index.js",
    "build:storybook": "vue-cli-service storybook:build -c config/storybook",
    "serve:storybook": "vue-cli-service storybook:serve -p 6006 -c config/storybook",
    "test:unit": "vue-cli-service test:unit",
    "test:unit:watch": "npm run test:unit -- --watch",
    "test:unit:coverage": "npm run test:unit -- --coverage",
    "test:unit:ci": "npm run test:unit:coverage -- --ci",
    "deploy": "npm run build:storybook && node ./deploy/deploy.js $FOLDER"
  },
  "dependencies": {
    "lodash": "4.17.11",
    "register-service-worker": "1.0.0",
    "reset-css": "4.0.1",
    "vue": "2.5.17"
  },
  "devDependencies": {
    "@storybook/addon-actions": "4.1.0",
    "@storybook/addon-console": "1.1.0",
    "@storybook/addon-knobs": "4.0.9",
    "@storybook/addon-links": "4.1.0",
    "@storybook/addon-notes": "4.0.8",
    "@vue/cli-plugin-babel": "3.1.1",
    "@vue/cli-plugin-eslint": "3.1.5",
    "@vue/cli-plugin-pwa": "3.1.2",
    "@vue/cli-plugin-unit-jest": "^3.5.0",
    "@vue/cli-service": "3.1.4",
    "@vue/eslint-config-airbnb": "4.0.0",
    "@vue/test-utils": "1.0.0-beta.20",
    "aws-sdk": "^2.395.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "eslint": "5.8.0",
    "eslint-plugin-vue": "5.0.0",
    "html-loader": "0.5.5",
    "lint-staged": "7.2.2",
    "markdown-loader": "4.0.0",
    "node-sass": "^4.11.0",
    "s3-node-client": "^4.4.4",
    "sass-loader": "7.0.1",
    "vue-cli-plugin-storybook": "0.5.0",
    "vue-template-compiler": "2.5.17"
  },
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged",
    "pre-push": "npm run test:unit"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "main": "./dist/homeday-blocks.common.js"
}
