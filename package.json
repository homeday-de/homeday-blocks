{
  "name": "homeday-blocks",
  "version": "0.0.0-development",
  "description": "A Vue component library built by Homeday's frontend team.",
  "scripts": {
    "type-check": "vue-tsc --noEmit",
    "lint": "vue-cli-service lint",
    "lint:ci": "vue-cli-service lint --no-fix",
    "build:tsc": "tsc -p tsconfig-build.json",
    "build": "node scripts/build.js",
    "build:storybook": "build-storybook",
    "serve:storybook": "start-storybook -p 6006",
    "coverage": "cat ./coverage/lcov.info | coveralls -v",
    "new": "hygen new",
    "semantic-release": "semantic-release",
    "test:percy": "if [ $PERCY_TOKEN ]; then percy-storybook; else echo 'Skipping test: PERCY_TOKEN is not defined'; fi",
    "test:unit": "vue-cli-service test:unit",
    "test:unit:ci": "npm run test:unit -- --ci --coverage",
    "test:unit:coverage": "npm run test:unit -- --coverage && open coverage/lcov-report/index.html",
    "test:unit:snapshot": "vue-cli-service test:unit -u",
    "test:unit:watch": "vue-cli-service test:unit --watchAll",
    "preinstall": "npx npm-force-resolutions",
    "prepack": "npm run build",
    "prepare": "husky install"
  },
  "main": "main.js",
  "sideEffects": [
    "*.vue",
    "src/styles/main.scss",
    "src/stories/styles/stories.scss"
  ],
  "files": [
    "dist/*",
    "src/@types/*",
    "src/assets/*",
    "src/components/*",
    "src/lang/*",
    "src/services/*",
    "src/styles/*",
    "src/index.js",
    "*.json",
    "*.js"
  ],
  "dependencies": {
    "awesome-phonenumber": "^5.11.0",
    "body-scroll-lock": "3.1.5",
    "country-codes-list": "^2.0.0",
    "deepmerge": "^4.3.1",
    "homeday-assets": "^4.66.1",
    "lodash": "4.17.21",
    "vue": "2.7.16",
    "vue-flickity": "1.2.1",
    "vue-inline-svg": "2.1.4",
    "vue-zoomer": "git+https://git@github.com/homeday-de/vue-zoomer#0.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/eslint-parser": "^7.27.1",
    "@babel/runtime": "^7.27.1",
    "@percy/storybook": "3.3.1",
    "@storybook/addon-actions": "6.5.12",
    "@storybook/addon-console": "1.2.3",
    "@storybook/addon-essentials": "6.5.12",
    "@storybook/addon-knobs": "6.4.0",
    "@storybook/addon-links": "6.5.12",
    "@storybook/addon-styling": "^1.3.7",
    "@storybook/addon-viewport": "6.5.12",
    "@storybook/builder-webpack5": "^6.5.12",
    "@storybook/manager-webpack5": "^6.5.12",
    "@storybook/mdx2-csf": "^0.0.3",
    "@storybook/theming": "6.5.12",
    "@storybook/vue": "6.5.12",
    "@testing-library/jest-dom": "^5.17.0",
    "@types/jest": "^24.9.1",
    "@types/lodash": "^4.17.16",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vue/cli-plugin-babel": "~5.0.8",
    "@vue/cli-plugin-eslint": "~5.0.8",
    "@vue/cli-plugin-typescript": "~5.0.8",
    "@vue/cli-plugin-unit-jest": "~5.0.8",
    "@vue/cli-service": "~5.0.8",
    "@vue/eslint-config-airbnb": "^5.3.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^1.3.6",
    "@vue/vue2-jest": "^27.0.0-alpha.3",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.5",
    "core-js": "3.42.0",
    "coveralls": "^3.1.1",
    "dateformat": "^4.5.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^10.1.3",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.33.0",
    "gitmoji-cli": "7.0.2",
    "gitmojis": "^3.15.0",
    "husky": "8.0.3",
    "hygen": "^6.2.11",
    "jest": "^27.5.1",
    "p-limit": "3.1.0",
    "postcss": "^8.5.3",
    "prettier": "^2.8.8",
    "raw-loader": "4.0.2",
    "sass": "^1.87.0",
    "sass-loader": "10.5.2",
    "semantic-release": "^19.0.5",
    "semantic-release-gitmoji": "^1.6.8",
    "svg-url-loader": "^8.0.0",
    "ts-jest": "^27.1.5",
    "typescript": "~4.9.5",
    "vue-code-highlight": "^0.7.8",
    "vue-loader": "^15.9.8",
    "vue-router": "^3.6.5",
    "vue-template-compiler": "2.7.16",
    "vue-tsc": "1.2.1",
    "webpack": "^5.99.8",
    "npm-force-resolutions": "0.0.10"
  },
  "resolutions": {
    "proxy-agent": "6.5.0"
  },
  "bugs": {
    "url": "https://github.com/homeday-de/homeday-blocks/issues"
  },
  "homepage": "https://blocks.homeday.dev",
  "keywords": [
    "vue",
    "javascript",
    "component library",
    "storybook",
    "homeday-blocks",
    "services"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/homeday-de/homeday-blocks.git"
  }
}
